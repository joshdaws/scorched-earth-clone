{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "checkJs": true,
    "strict": false,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "/*": ["./*"]
    }
  },
  "include": [
    "js/**/*.js",
    "tests/**/*.js"
  ],
  "exclude": [
    "node_modules",
    "www",
    "js/vendor/**"
  ]
}
